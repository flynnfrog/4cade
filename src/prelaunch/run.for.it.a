!cpu 6502
!to "build/PRELAUNCH/RUN.FOR.IT",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $9DC
         jsr   $800       ; load DOS
         lda   #$60
         sta   $B727
         jsr   $B700      ; load game
         +GET_MACHINE_STATUS
         and   #CHEATS_ENABLED
         beq   +
         lda   #$a5
         sta   $906
         sta   $1286
+
         +DISABLE_ACCEL
         jmp   $800

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

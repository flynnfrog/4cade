!cpu 6502
!to "build/PRELAUNCH/WOLFEN84",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         +USES_TEXT_PAGE_2
         lda   #$60
         sta   $21E8
         jsr   $2000       ; find and load "DOS"
         lda   #<callback
         sta   $B748
         lda   #>callback
         sta   $B749
         jmp   $B700

reset
         +READ_ROM_NO_WRITE
         jmp   $FAA6

callback
         lda   #>reset
         sta   $9E52
         lda   #<reset
         sta   $9E53      ; reset vector patch

         +DISABLE_ACCEL
         jmp   $9D84

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

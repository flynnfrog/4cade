!cpu 6502
!to "build/PRELAUNCH/A.E",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $4D23
         bne   +
         !fill $15
+        jsr   $BD5       ; decompress
         +GET_MACHINE_STATUS

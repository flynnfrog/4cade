!cpu 6502
!to "build/PRELAUNCH/IMPOSSIBLE.MISS",plain
*=$106

    !source "src/prelaunch/common.a"

         lda   #$60
         sta   $920
         jsr   $800       ; decompress

         lda   #$60
         sta   $9f1
         jsr   $9AD
         jsr   $9050

         lda   #$60
         sta   $4018
         jsr   $4000      ; LC switched in

         lda   #$82
         sta   $101
         lda   #$A6
         sta   $104
         lda   #$FA
         sta   $105       ; update TR hook to reboot
         lda   #0
         sta   $FFFC
         lda   #1
         sta   $FFFD      ; reset vector fix

         jmp   $800

!if * > $1C0 {
  !error "code is too large, ends at ", *
}

!cpu 6502
!to "build/PRELAUNCH/LAST.GLADIATOR",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $11EA
         jsr   $1000      ; decompress

         lda   #$60
         sta   $BC00

         +DISABLE_ACCEL
         jmp   $11EF

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
